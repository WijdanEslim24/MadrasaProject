    <!-- Title of Course Upgrade Error Alert -->
    <string name="title_upgrade_error">אירעה שגיאה</string>
    <!-- Message for course upgrade error -->
    <string name="general_error_message">נראה שמשהו השתבש בשדרוג הקורס שלך. אם השגיאה הזאת נמשכת, נא ליצור קשר עם התמיכה.</string>
    <!-- Email Subject for Course Upgrade Error -->
    <string name="email_subject_upgrade_error">שגיאה בשדרוג הקורס ביישומון</string>
    <!-- Get Help button label -->
    <string name="label_get_help">קבלת עזרה</string>
    <!-- Refresh To Retry button label -->
    <string name="label_refresh_to_retry">יש לרענן כדי לנסות שוב</string>

    <!--  Error Messages  -->
    <!--  Course didn't found against the course sku in the course upgrade API -->
    <string name="error_course_not_found">הקורס שברצונך לשדרג לא נמצא. נא לנסות את השדרוג שלך שוב. אם השגיאה הזאת נמשכת, נא ליצור קשר עם התמיכה.</string>
    <!-- Authentication error in course upgrade API's basket, checkout, execute  -->
    <string name="error_user_not_authenticated">לא ניתן לאמת את החשבון שלך. כדאי לנסות לצאת ולהיכנס ליישומון בחזרה. אם השגיאה הזאת נמשכת, נא ליצור קשר עם התמיכה.</string>
    <!--  The course is already upgraded  -->
    <string name="error_course_already_paid">הקורס שברצונך לשדרג כבר שולם. כדי לעדכן, אנחנו צריכים לרענן את היישומון שלך בזריזות.</string>
    <!--  Course sku or the payment processor didn't found  -->
    <string name="error_payment_not_processed">לא ניתן לעבד את התשלום שלך כרגע. נא לנסות שוב. לעזרה נוספת, נא לפנות לתמיכה.</string>
    <!--  The native payment is complete but an error occurred while marking the course verified  -->
    <string name="error_course_not_fullfilled">משהו קרה כשניסינו לעדכן את חוויית הקורס שלך. אם השגיאה הזאת נמשכת, נא לפנות לתמיכה לעזרה.</string>
    <!--  The price wasn't fetched from the payment sdk  -->
    <string name="error_price_not_fetched">לא ניתן להשלים את הבקשה שלך כרגע. אם השגיאה הזאת נמשכת, נא לפנות לתמיכה.</string>
    <!--  The course is upgraded and verified  -->
    <string name="error_course_paid_and_verified">הקורס שברצונך לשדרג כבר שולם. לעזרה נוספת, נא לגשת לתמיכה.</string>


